<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="Tenant" xmlns="http://ws.apache.org/ns/synapse">
    <address optimize="swa" statistics="enable" uri="http://localhost:8081/graphql">
        <enableSec/>
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </address>
</endpoint>
